{
  "cli": {
    "version": ">= 16.4.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "env": {
        "EXPO_PUBLIC_BUILD_TYPE": "development"
      },
      "android": {
        "buildType": "apk"
      },
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://prepgpt-backend-orl9.onrender.com/api",
        "EXPO_PUBLIC_BUILD_TYPE": "preview"
      }
    },
    "internal": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL": "https://prepgpt-backend-orl9.onrender.com/api",
        "EXPO_PUBLIC_BUILD_TYPE": "internal"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "android": {
        "buildType": "app-bundle"
      },
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_API_URL": "https://prepgpt-backend-orl9.onrender.com/api",
        "EXPO_PUBLIC_BUILD_TYPE": "production"
      }
    }
  }
}
